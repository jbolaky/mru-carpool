<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
	version="1.0">
	<persistence-unit name="location_PersistenceUnitName"
		transaction-type="RESOURCE_LOCAL">
		<provider>org.hibernate.ejb.HibernatePersistence</provider>
		<class>com.javaid.bolaky.domain.location.entity.Country</class>
		<class>com.javaid.bolaky.domain.location.entity.Area</class>
		<class>com.javaid.bolaky.domain.location.entity.District</class>
	</persistence-unit>
</persistence>